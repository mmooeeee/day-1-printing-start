#function with more tan 1 input
def greet_with(name, location):
  print(f"Hello {name}")
  print(f"what is it like in {location}?")

#greet_with("Moe", "Tangerang")

#function with keyword argumnt
greet_with(location="Tangerang", name="Moe")
